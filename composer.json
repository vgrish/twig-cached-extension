{
    "name": "manubo/twig-cached-extension",
    "description": "Twig extension for caching html fragments",
    "keywords": ["twig", "extension", "cached", "cache"],
    "homepage": "https://github.com/manubo/twig-cached-extension",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Manuel Bouza",
            "email": "ma75bo@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/manubo/psr-cache"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Crell/Cache"
        }
    ],
    "require": {
        "php": ">=5.4.2",
        "twig/twig": "~1.15",
        "psr/cache": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "manubo/psr-cache": "dev-master"
    },
    "autoload": {
        "psr-4": { "Manubo\\Twig\\": "src/Manubo/Twig" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}

